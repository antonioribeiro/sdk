  "require": {
    "php": ">=7.1",
    "laravel/framework": ">=5.3",
    "pragmarx/tracker": "~3.1",
    "pragmarx/firewall": "~1.0",
    "pragmarx/glottos": "~1.5",
    "pragmarx/zipcode": "~1.5",
    "pragmarx/google2fa": "~1.0",
    "guzzlehttp/guzzle": "~6.0",
    "cartalyst/sentinel": "*",
    "cartalyst/support": "*",
    "twilio/sdk":"3.12.*",
    "jenssegers/date": "~3",
    "league/commonmark": "~0.9",
    "baum/baum": "~1.1",
    "laravelcollective/html": "^5.3",
    "watson/rememberable": "^1.0",
    "h4kuna/number-format": "1.2.*",
    "fabpot/goutte": "~3.1",
    "thiagoalessio/tesseract_ocr": "1.0.0-RC"
