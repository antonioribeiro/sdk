  "require-dev": {
    "phpspec/phpspec": "2.0.*@dev",
    "codeception/codeception": "2.0.*@dev",
    "behat/behat": "3.0.*",
    "mockery/mockery": "~0.8",
    "laracasts/testdummy": "1.*",
    "fzaninotto/faker": "1.4.*",
    "pragmarx/sqli": "0.*"
